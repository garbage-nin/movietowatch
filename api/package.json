{
  "name": "api-movietowatch",
  "version": "1.0.0",
  "description": "Restful API with nodejs,express, mongodb",
  "main": "server.js",
  "scripts": {
    "start": "npm run import-db && nodemon server.js",
    "import-db": "mongoimport --drop --db=movietowatch --collection=movies --file=data/movies.json --jsonArray",
    "export-db": "mongoexport --db=movietowatch --collection=movies --out=data/movies.json --jsonArray",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "rest",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.6.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.1"
  }
}
